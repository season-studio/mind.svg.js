{
    "extends": [
        "eslint:recommended"
    ],
    "env":{
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "rules": {
        "semi": ["error", "always"],
        "quotes": "off",
        "no-console": "off",
        "no-unused-vars": "warn",
        "no-unreachable": "warn",
        "no-redeclare": "warn"
    }
}